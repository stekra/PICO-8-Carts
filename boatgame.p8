 end
 g.update=function(this)
  if (g.y>-v+128+g.yoff) then
   yoff=rnd(30)
   g.x=rnd(85)+2
	  g.y=-v-53-g.yoff
	  g.passed=false
	 end
	 if (b.y<g.y) then
	 	if (b.x+4>g.x and b.x<g.x+32) then
	 	 if(not g.passed) then
	 	  g.pass(this)
	 	  b.goals+=1
