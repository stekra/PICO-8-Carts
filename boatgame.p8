-- draw map

function draw_map()
 for x=1,62 do
	 for y=1,62 do
	  -- bottom
	  if grid[x][y]==1 then
	   if  grid[x-1][y]~=1 
	   and grid[x][y+1]~=1 then
	    mset(x,y,52) -- corner l
	   elseif grid[x-1][y]==1
	   and    grid[x+1][y]==1
	   and    grid[x][y+1]~=1 then
	    mset(x,y,53) -- front
	   elseif grid[x+1][y]~=1
	   and    grid[x][y+1]~=1 then
	    mset(x,y,54) -- corner r
	   end
	  end
	  -- middle
	  if grid[x][y+1]==1 then
		  if grid[x-1][y+1]~=1
		  and grid[x-1][y]~=1 then
		   mset(x,y,36) -- left
		  elseif grid[x+1][y+1]~=1
		  and    grid[x+1][y]~=1 then
		   mset(x,y,38) -- right
		  else
		   mset(x,y,37) -- mid
		  end
		 -- top
		 elseif grid[x][y+2]==1 then
		  if grid[x-1][y+2]~=1
		  and grid[x-1][y]~=1 then
		   mset(x,y,20) -- left
		  elseif grid[x+1][y+2]~=1
		  and    grid[x+1][y]~=1 then
		   mset(x,y,22) -- right
		  else
		   mset(x,y,21) -- mid
		  end
		 end
	 end
	end
	
	-- peaks
	for x=1,62 do
	 for y=1,62 do
	  if grid[x][y]~=1 then
		  if mget(x,y+1)==20 then
		   mset(x,y,4) -- left
		  elseif mget(x,y+1)==21 then
		   mset(x,y,5)
		  elseif mget(x,y+1)==22 then
		   mset(x,y,6)
		  end
		 end
	 end
	end
	
end

