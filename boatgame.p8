function draw_boat(x,y,a)
 for i=0,1 do
  local sx=16*(i%8)
  local sy=16*flr(i/8)
  rspr(sx,sy,x,y-i,a,2)
 end
end

function draw_sail(x,y,a)
 local ax=cos(a)
 local ay=sin(a)
 local p1={x,y-2}
 local p2={x,y-11}
 local p3={x+ax*8,y-2-ay*8}
 tri(p1,p2,p3,7)
 line(x,y-1,x,y-11,5)
end

function draw_sail_refl(x,y,a)
 pal(7,10)
 pal(5,11)
 local ax=cos(a)
 local ay=sin(a)
 local p1={x,y+2}
 local p2={x,y+11}
 local p3={x+ax*8,y+2-ay*8}
 tri(p1,p2,p3,7)
 line(x,y,x,y+11,5)
 pal()
 pal(10,138,1)
end

