function _init()
	b={} -- boat
	b.x=32*8
	b.y=32*8
	b.vx=0
	b.vy=0
	b.ang=0.25
	b.spd=0.002
	b.goals=0
	b.c=false -- alt color
	b.cs=0 -- color step
	b.col=0 -- collided
	b.col_max=2
	
	s={} -- sail
	s.x=b.x
	s.y=b.y
	s.ang=.35
	
	g=init_gull()
	
	cam={}
	cam.x=0
	cam.y=0
	
	sox={}
	
	wang=0.5 -- wind angle
	
	vel=0
	
	-- setup map
	grid={}
	for i=0,63 do
	 grid[i]={}
	 for j=0,63 do
	  if (i==0 or i==63) then
	   grid[i][j]=2
	  elseif (j==0 or j==63) then
	   grid[i][j]=2
	  else
	   grid[i][j]=0
	  end
	 end
	end
	
	os2d_noise(rnd(128))
	
	for y=0,62 do
	 for x=0,62 do
	  local val=0
	  val+=os2d_eval(x/5,y/5)
	  val+=os2d_eval(x/2,y/2)*0.4
	  if (val>0.5) then
	   grid[x][y]=1
	   grid[x+1][y]=1
	  end
	 end
	end
	
	g.init()
	
	setup_map()
--	setup_map_refl()
		
 printh("\n\n-----run------\n")
 
 sok=new_sock(117,12)
end

