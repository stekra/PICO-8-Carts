function _init()
	b={} -- boat
	b.x=40
	b.y=60*8
	b.vx=0
	b.vy=0
	b.ang=0.25
	b.spd=0.002
	b.goals=0
	
	s={} -- sail
	s.x=b.x
	s.y=b.y
	s.ang=.35
	
	g={} -- gull
	g.x=0
	g.y=0
	g.tx=b.x
	g.ty=b.y
	g.r=30 -- fly radius
	g.s=35 -- fly speed
	g.spr={32,33,34,35}
	g.step=0 -- anim step
	g.fr=1   -- anim frame
	g.ft={2,1,1,1} -- rel fr time
	g.update=function()
	 g.step+=1
	 local t=g.step%(10*g.ft[g.fr])
	 if (t==0) g.fr+=1
	 if (g.fr>4) g.fr=1
	 g.tx=b.x
	 g.ty=b.y
	 local lx=sin(time()*4/g.s)
	 local ly=cos(time()*5/g.s)
	 g.x=g.tx+lx*g.r
	 g.y=g.ty+ly*g.r
	end
	g.draw=function()
--	 circfill(g.x,g.y,3,7)
  spr(g.spr[g.fr],g.x,g.y)
	end
	
	cam={}
	cam.x=0
	cam.y=0
	
	wang=0.5 -- wind angle
	
	vel=0
	
	-- setup map
	grid={}
	for i=0,63 do
	 grid[i]={}
	 for j=0,63 do
	  if (i==0 or i==63) then
	   grid[i][j]=2
	  elseif (j==0 or j==63) then
	   grid[i][j]=2
	  else
	   grid[i][j]=0
	  end
	 end
	end
	
	os2d_noise(rnd(128))
	
	for y=0,62 do
	 for x=0,62 do
	  local val=0
	  val+=os2d_eval(x/5,y/5)
	  val+=os2d_eval(x/2,y/2)*0.4
	  if (val>0.55) then
	   grid[x][y]=1
	   grid[x+1][y]=1
	  end
	 end
	end
	
	draw_map()
		
 printh("\n\n-----run------\n")
end

function _update60()
 b.ang=b.ang%1
 s.ang=s.ang%1
 
 wf=abs(sin(s.ang-wang))
 
 b.vx-=cos(b.ang)*b.spd*wf
 b.vy+=sin(b.ang)*b.spd*wf
 
 falloff=0.99
 b.vx*=falloff
 b.vy*=falloff
 
 b.x+=b.vx
 b.y+=b.vy

 vel+=b.spd*wf
 vel*=0.995
 
 b.x-=cos(b.ang)*vel/2
 b.y+=sin(b.ang)*vel/2
 
 -- turn ctrl
 t=0.005
 spd=sqrt(b.vx*b.vx+b.vy*b.vy)
 spd=(spd*2.5+0.2)
 if (btn(⬅️)) b.ang-=t*spd
 if (btn(➡️)) b.ang+=t*spd
 
 -- sail ctrl
 if (btn(🅾️) or btn(❎)) then
  s.ang=arot(s.ang,b.ang,0.0027)
 else
  s.ang=arot(s.ang,wang,0.003)
	end
	 
 -- sail root position
-- s.x=b.x-cos(b.ang)*2+7.5
-- s.y=b.y+sin(b.ang)*2+7.5
 s.x=b.x+8
 s.y=b.y+8

