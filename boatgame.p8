-- draw functions ‚àß

function draw_boat(x,y,a)
 for i=0,1 do
  local sx=16*(i%8)
  local sy=16*flr(i/8)
  rspr(sx,sy,x,y-i,a,2)
 end
end

function draw_sail(x,y,a)
 for i=2,11 do
  local sx=16*(i%8)
  local sy=16*flr(i/8)
  rspr(sx,sy,x,y-i,a,2)
 end
end

function draw_sail_refl(x,y,a)
 for i=2,11 do
  local sx=16*(i%8)
  local sy=16*flr(i/8)
  pal(7,10)
  pal(5,11)
--  rspr(sx,sy,x+sin(i*time()/10),y+i,a,2)
  rspr(sx,sy,x,y+i,a,2)
  pal()
  pal(10,138,1)
 end
-- for i=2,30,2 do
--  line(x,y+i,x+16,y+i,3)
-- end
end

function draw_start(y)
 print("‚óè‚Ä¶‚Ä¶‚Ä¶",0,y+v,11)
	print("‚Ä¶‚Ä¶‚Ä¶‚Ä¶",32,y+v,11)
	print("‚Ä¶‚Ä¶‚Ä¶‚Ä¶",64,y+v,11)
	print("‚Ä¶‚Ä¶‚Ä¶‚óè",96,y+v,11)
	for i=0,4,2 do
		print("‚óè",0,y-1-i+v,10)
	 print("‚óè",0,y-2-i+v,0)
	 print("‚óè",120,y-1-i+v,10)
	 print("‚óè",120,y-2-i+v,0)
	end
end
