function new_sock(x,y)
 local s={}
 s.r=2
 s.x=x
 s.y=y
 s.a=wang
 s.off=0.75
 s.len=10
 s.col={9,9,7,7,9,9,7,7,9,9}
 s.update=function(this)
		s.a=wang+sin(time()*2)/80
 end
 s.draw=function(this)
  if s.a>0.5 then
	  for i=this.len,1,-1 do
	   color(s.col[i])
	   local r=s.r
	   if (i>5) r=s.r-1
		  ovalfill(
		   s.x+cos(s.a)*s.off*i-sin(s.a)*r,
	    s.y-sin(s.a)*s.off*i-r,
	    s.x+cos(s.a)*s.off*i+sin(s.a)*r,
