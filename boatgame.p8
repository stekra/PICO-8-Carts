function _init()
	b={} -- boat
	b.x=40
	b.y=100
	b.vx=0
	b.vy=0
	b.ang=0.25
	b.spd=0.004
	
	s={} -- sail
	s.x=b.x
	s.y=b.y
	s.ang=.35
	
	wang=0.5 -- wind angle
	
	v=0 -- vertical progress
	
	-- gates
	gx1=rnd(85)+2
	gy1=rnd(30)
	gx2=rnd(85)+2
	gy2=-50+rnd(10)
end

function _update60()
 b.ang=b.ang%1
 s.ang=s.ang%1
 
 wf=abs(sin(s.ang-wang))
 
 b.vx-=cos(b.ang)*b.spd*wf
 b.vy+=sin(b.ang)*b.spd*wf
 
 falloff=0.99
 b.vx*=falloff
 b.vy*=falloff
 -- todo: prevent sliding
 -- somehow...
 
 b.x+=b.vx
 b.y+=b.vy
 
 -- turn ctrl
 t=0.004
 spd=sqrt(b.vx*b.vx+b.vy*b.vy)
 spd=(spd*2.5+0.2)
 if (btn(‚¨ÖÔ∏è)) b.ang-=t*spd
 if (btn(‚û°Ô∏è)) b.ang+=t*spd
 
 -- sail ctrl
 if (btn(üÖæÔ∏è)) then
  s.ang=arot(s.ang,b.ang,0.0027)
 else
  s.ang=arot(s.ang,wang,0.003)
	end
	 
 -- sail root position
 local offset=2
 s.x=b.x-cos(b.ang)*offset
 s.x-=0.5
 s.y=b.y+sin(b.ang)*offset
 
 -- turn wind
 wang=(wang+0.0001)%1

 -- scroll map
 if (b.y+v<64) v+=0.3 -- top
 if (b.y+v<32) v+=0.5 -- fast
 if (b.y+v>90) v-=0.2 -- back
end

function _draw()
	cls(3)
	color(11)
	print("a:"..b.ang,0,0)
 print("x:"..b.x,0,6)
	print("y:"..b.y,0,12)
	print("sa:"..s.ang,0,20)
	print("wf:"..wf,0,28)
	print("spd:"..spd,0,34)
	
	draw_sail_refl(s.x,s.y+v,s.ang)

 draw_start(80)
 gate1()
 gate2()
 
 draw_boat(b.x,b.y+v,b.ang)
 draw_sail(s.x,s.y+v,s.ang)
 
 -- wind
 rspr(64,16,108,8,wang,2)
 print("wind",108,1,12)
 print(wang,105,26,12)
end


function gate1()
 print("‚óè‚Ä¶‚Ä¶‚Ä¶‚óè",gx1,gy1+v+1,11)
 print("‚óè      ‚óè",gx1,gy1+v,9)
 if (gy1>-v+128) then
  gx1=rnd(85)+2
	 gy1=-v-1-rnd(30)
 end
end

function gate2()
 print("‚óè‚Ä¶‚Ä¶‚Ä¶‚óè",gx2,gy2+v+1,11)
 print("‚óè      ‚óè",gx2,gy2+v,14)
 if (gy2>-v+128) then
  gx2=rnd(85)+2
	 gy2=-v-1-rnd(30)
 end
end

