 local cx=flr((b.x+8)/8)
 local cy=flr((b.y+8)/8)
 if grid[cx][cy]==1 then
  printh("collided "..cx.." "..cy)
 end
 
 g.update()
 
 st=stat(1)
 fps=stat(7)
end

