  if not g.sitting then
	  spr(g.spr[g.fr],g.x,g.y-g.h)
	 else
	  spr(g.sspr[g.sfr],g.tx,g.ty-20)
	 end
	end
	g.draw_refl=function()
	 if not g.sitting then
	  pal(7,10)
	  spr(g.spr[g.fr],g.x,g.y+g.h,1,1,false,true)
	  pal()
	 end
	end
	
	cam={}
	cam.x=0
	cam.y=0
	
	sox={}
	
	wang=0.5 -- wind angle
	
	vel=0
	
	-- setup map
	grid={}
	for i=0,63 do
